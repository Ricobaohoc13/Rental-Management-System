      <Button layoutX="275.0" layoutY="312.0" mnemonicParsing="false" onAction="#LogIn" text="Log In" />

      <!-- Added fx:id "Verification" -->
      <TextField fx:id="Verification" layoutX="220.0" layoutY="225.0" promptText="Verification Code" />

      <Label layoutX="449.0" layoutY="137.0" text="Verification Code:" />

      <!-- Added fx:id "infoLabel" -->
      <Label fx:id="infoLabel" layoutX="482.0" layoutY="192.0" text="code: 1234" />

      <Label layoutX="286.0" layoutY="60.0" text="Log In" />

      <Button layoutX="29.0" layoutY="35.0" mnemonicParsing="false" onAction="#GoBack" text="Go Back" />
      <ComboBox fx:id="Role" layoutX="220.0" layoutY="267.0" prefWidth="150.0" promptText="Role" />
